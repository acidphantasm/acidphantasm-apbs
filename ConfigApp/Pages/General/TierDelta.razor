                    <MudItem xs="6">
                        <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                            <MudSwitch T="bool" @bind-Value="_enableCustomLevelDeltas" Color="Color.Success" UncheckedColor="Color.Error" LabelPlacement="Placement.End">@_enableCustomLevelDeltas</MudSwitch>
