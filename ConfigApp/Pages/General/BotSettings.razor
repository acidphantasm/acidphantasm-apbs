                            <MudNumericField Margin="Margin.Dense"
                                             HideSpinButtons="true"
                                             @bind-Value="_mapRangeWeighting_Customs_Long"
                                             Label="Weight"
                                             HelperText="Long Range"
                                             Variant="Variant.Filled"
                                             Min="0"
                                             Max="100" />
