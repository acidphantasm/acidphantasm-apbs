@page "/botsettings"
@inherits LayoutComponentBase
@using static Core.DataLoader;

<MudMainContent>
    <MudContainer>
        <MudExpansionPanels MultiExpansion="true">
            <MudExpansionPanel Class="mud-expansion-panels-borders ma-3 pa-5" Text="Weapon & Armour Configurations" Expanded=true>
                <MudGrid>

                    <MudItem xs="1" />
                    <MudItem xs="4">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Allows bots to roll their selected round from their weapons caliber, max one time per each magazine." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                                <MudText>Reroll Selected Ammunition</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="6">
                        <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                            <MudSwitch T="bool" @bind-Value="Data.enableBotsToRollAmmoAgain" Color="Color.Success" UncheckedColor="Color.Error" LabelPlacement="Placement.End">@Data.enableBotsToRollAmmoAgain</MudSwitch>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton @onclick="resetRollAmmoAgain" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Reset</MudButton>
                    </MudItem>

                    <MudItem xs="1" />
                    <MudItem xs="4">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="This is the chance that bots will roll for a different round from the same caliber, max one time per each magazine." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                                <MudText>Chance to Reroll for Each Magazine</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small" Disabled="!Data.enableBotsToRollAmmoAgain">@Data.chanceToRollAmmoAgain</MudButton>
                    </MudItem>
                    <MudItem xs="5">
                        <MudSlider @bind-Value="Data.chanceToRollAmmoAgain" ValueLabel="true" Min="1" Max="100" Color="Color.Info" Size="Size.Medium" Disabled="!Data.enableBotsToRollAmmoAgain" />
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton @onclick="resetRollAmmoChance" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small" Disabled="!Data.enableBotsToRollAmmoAgain">Reset</MudButton>
                    </MudItem>

                    <MudItem xs="1" />
                    <MudItem xs="4">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Allows PMCs & Scavs to have random usage on Food/Drink/Medical Items." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                                <MudText>Enable Resource Randomization</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="6">
                        <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                            <MudSwitch T="bool" @bind-Value="Data.enableConsumableResourceRandomization" Color="Color.Success" UncheckedColor="Color.Error" LabelPlacement="Placement.End">@Data.enableConsumableResourceRandomization</MudSwitch>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton @onclick="resetResourceRandomization" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Reset</MudButton>
                    </MudItem>

                    <MudItem xs="2" />
                    <MudItem xs="3">
                        <MudTooltip Text="PMC Medical & Food rates." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                            <MudText>PMC Medical & Food</MudText>
                        </MudTooltip>
                    </MudItem>
                    <MudItem xs="7">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudNumericField @bind-Value="pmcMedRates[0]" Label="Med Full Chance" Variant="Variant.Filled" Min="1" Max="100" Disabled="!Data.enableConsumableResourceRandomization" />
                            <MudNumericField @bind-Value="pmcMedRates[1]" Label="Med Usage" Variant="Variant.Filled" Min="1" Max="100" Disabled="!Data.enableConsumableResourceRandomization" />
                            <MudNumericField @bind-Value="pmcFoodRates[0]" Label="Food Full Chance" Variant="Variant.Filled" Min="1" Max="100" Disabled="!Data.enableConsumableResourceRandomization" />
                            <MudNumericField @bind-Value="pmcFoodRates[1]" Label="Food Usage" Variant="Variant.Filled" Min="1" Max="100" Disabled="!Data.enableConsumableResourceRandomization" />
                            <MudSpacer />
                        </MudPaper>
                    </MudItem>

                    <MudItem xs="2" />
                    <MudItem xs="3">
                        <MudTooltip Text="Scav Medical & Food rates." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                            <MudText>Scav Medical & Food</MudText>
                        </MudTooltip>
                    </MudItem>
                    <MudItem xs="7">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudNumericField @bind-Value="scavMedRates[0]" Label="Med Full Chance" Variant="Variant.Filled" Min="1" Max="100" Disabled="!Data.enableConsumableResourceRandomization" />
                            <MudNumericField @bind-Value="scavMedRates[1]" Label="Med Usage" Variant="Variant.Filled" Min="1" Max="100" Disabled="!Data.enableConsumableResourceRandomization" />
                            <MudNumericField @bind-Value="scavFoodRates[0]" Label="Food Full Chance" Variant="Variant.Filled" Min="1" Max="100" Disabled="!Data.enableConsumableResourceRandomization" />
                            <MudNumericField @bind-Value="scavFoodRates[1]" Label="Food Usage" Variant="Variant.Filled" Min="1" Max="100" Disabled="!Data.enableConsumableResourceRandomization" />
                        </MudPaper>
                    </MudItem>

                    <MudItem xs="1" />
                    <MudItem xs="4">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Allows bots to use different chances for each slot, depending on the category of weapon they have selected." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                                <MudText>Per Weapon Category Attachment Chances</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="6">
                        <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                            <MudSwitch T="bool" @bind-Value="Data.enablePerWeaponTypeAttachmentChances" Color="Color.Success" UncheckedColor="Color.Error" LabelPlacement="Placement.End">@Data.enablePerWeaponTypeAttachmentChances</MudSwitch>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton @onclick="resetPerWeaponAttachments" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Reset</MudButton>
                    </MudItem>

                    <MudItem xs="1" />
                    <MudItem xs="4">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Forces bots to use a stock if their weapon allows it." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                                <MudText>Forced Stock</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="6">
                        <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                            <MudSwitch T="bool" @bind-Value="Data.forceStock" Color="Color.Success" UncheckedColor="Color.Error" LabelPlacement="Placement.End">@Data.forceStock</MudSwitch>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton @onclick="resetForceStock" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Reset</MudButton>
                    </MudItem>

                    <MudItem xs="1" />
                    <MudItem xs="4">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="This is the chance that bot weapons, that have a stock with an available buttpad have one." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                                <MudText>Chance for Buttpad to Spawn on Stock</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">@Data.stockButtpadChance</MudButton>
                    </MudItem>
                    <MudItem xs="5">
                        <MudSlider @bind-Value="Data.stockButtpadChance" ValueLabel="true" Min="1" Max="100" Color="Color.Info" Size="Size.Medium" />
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton @onclick="resetButtpadChance" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Reset</MudButton>
                    </MudItem>

                    <MudItem xs="1" />
                    <MudItem xs="4">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Forces bots to use a dust cover if their weapon allows it." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                                <MudText>Forced Dust Cover</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="6">
                        <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                            <MudSwitch T="bool" @bind-Value="Data.forceDustCover" Color="Color.Success" UncheckedColor="Color.Error" LabelPlacement="Placement.End">@Data.forceDustCover</MudSwitch>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton @onclick="resetForceDustcover" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Reset</MudButton>
                    </MudItem>

                    <MudItem xs="1">
                        <MudPaper Class="d-flex justify-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Due to BSG - this may also caused unwanted tactical attachments" Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                                <MudIconButton Icon="@Icons.Material.Filled.Warning" Size="Size.Small" />
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="4">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Forces bots to use an optic if their weapon allows it." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                                <MudText>Forced Optic</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="6">
                        <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                            <MudSwitch T="bool" @bind-Value="Data.forceScopeSlot" Color="Color.Success" UncheckedColor="Color.Error" LabelPlacement="Placement.End">@Data.forceScopeSlot</MudSwitch>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton @onclick="resetForceScopeSlot" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Reset</MudButton>
                    </MudItem>

                    <MudItem xs="1"/>
                    <MudItem xs="4">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Override default database and preset chances of having a muzzle, if their weapon allows it." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                                <MudText>Override Muzzle Chances</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="6">
                        <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                            <MudSwitch T="bool" @bind-Value="Data.forceMuzzle" Color="Color.Success" UncheckedColor="Color.Error" LabelPlacement="Placement.End">@Data.forceMuzzle</MudSwitch>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton @onclick="resetForceMuzzle" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Reset</MudButton>
                    </MudItem>

                    <MudItem xs="2" />
                    <MudItem xs="3">
                            <MudTooltip Text="Forces bots to use a muzzle if their weapon allows it." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                                <MudText>Muzzle Chance</MudText>
                            </MudTooltip>
                    </MudItem>
                    <MudItem xs="7">                        
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudNumericField @bind-Value="muzzleChanceHolder[0]" Label="Tier 1" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.forceMuzzle" />
                            <MudNumericField @bind-Value="muzzleChanceHolder[1]" Label="Tier 2" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.forceMuzzle" />
                            <MudNumericField @bind-Value="muzzleChanceHolder[2]" Label="Tier 3" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.forceMuzzle" />
                            <MudNumericField @bind-Value="muzzleChanceHolder[3]" Label="Tier 4" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.forceMuzzle" />
                            <MudNumericField @bind-Value="muzzleChanceHolder[4]" Label="Tier 5" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.forceMuzzle" />
                            <MudNumericField @bind-Value="muzzleChanceHolder[5]" Label="Tier 6" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.forceMuzzle" />
                            <MudNumericField @bind-Value="muzzleChanceHolder[6]" Label="Tier 7" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.forceMuzzle" />
                        </MudPaper>
                    </MudItem>

                    <MudItem xs="1"/>
                    <MudItem xs="4">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="If a muzzle is selected for their weapon, this will force it to have it's children attachments (muzzle device -> suppressor)." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                                <MudText>Forced Muzzle Child Attachments</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="6">
                        <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                            <MudSwitch T="bool" @bind-Value="Data.forceChildrenMuzzle" Color="Color.Success" UncheckedColor="Color.Error" LabelPlacement="Placement.End">@Data.forceChildrenMuzzle</MudSwitch>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton @onclick="resetForceChildMuzzle" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Reset</MudButton>
                    </MudItem>

                    <MudItem xs="1"/>
                    <MudItem xs="4">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Limits weapons to specified number of optics & tactical devices listed below." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                                <MudText>Enforce Weapon Attachment Limits</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="6">
                        <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                            <MudSwitch T="bool" @bind-Value="Data.forceWeaponModLimits" Color="Color.Success" UncheckedColor="Color.Error" LabelPlacement="Placement.End">@Data.forceWeaponModLimits</MudSwitch>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton @onclick="resetForceWeaponModLimits" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Reset</MudButton>
                    </MudItem>
                    
                    <MudItem xs="2" />
                    <MudItem xs="3">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Limit to number of optics on weapons." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                                <MudText>Optic Device Limit</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small" Disabled="!Data.forceWeaponModLimits">@Data.scopeLimit</MudButton>
                    </MudItem>
                    <MudItem xs="6">
                        <MudSlider @bind-Value="Data.scopeLimit" ValueLabel="true" Min="1" Max="3" Color="Color.Info" Size="Size.Medium" Disabled="!Data.forceWeaponModLimits" />
                    </MudItem>
                    
                    <MudItem xs="2" />
                    <MudItem xs="3">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Limit to number of tactical devices on weapons." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                                <MudText>Tactical Device Limit</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small" Disabled="!Data.forceWeaponModLimits">@Data.tacticalLimit</MudButton>
                    </MudItem>
                    <MudItem xs="6">
                        <MudSlider @bind-Value="Data.tacticalLimit" ValueLabel="true" Min="1" Max="3" Color="Color.Info" Size="Size.Medium" Disabled="!Data.forceWeaponModLimits"/>
                    </MudItem>

                    <MudItem xs="1" />
                    <MudItem xs="4">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Override default database and preset chances of having a plates, if their armour allows it." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                                <MudText>Override Plate Chances</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="6">
                        <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                            <MudSwitch T="bool" @bind-Value="Data.enableCustomPlateChances" Color="Color.Success" UncheckedColor="Color.Error" LabelPlacement="Placement.End">@Data.enableCustomPlateChances</MudSwitch>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton @onclick="resetCustomPlateChances" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Reset</MudButton>
                    </MudItem>

                    <MudItem xs="2" />
                    <MudItem xs="3">
                        <MudTooltip Text="Sets PMC front/back plate chances per tier." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                            <MudText>PMC Main Plate Chances</MudText>
                        </MudTooltip>
                    </MudItem>
                    <MudItem xs="7">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudNumericField @bind-Value="pmcMainPlateChance[0]" Label="Tier 1" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="pmcMainPlateChance[1]" Label="Tier 2" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="pmcMainPlateChance[2]" Label="Tier 3" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="pmcMainPlateChance[3]" Label="Tier 4" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="pmcMainPlateChance[4]" Label="Tier 5" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="pmcMainPlateChance[5]" Label="Tier 6" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="pmcMainPlateChance[6]" Label="Tier 7" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                        </MudPaper>
                    </MudItem>

                    <MudItem xs="2" />
                    <MudItem xs="3">
                        <MudTooltip Text="Sets PMC left/right plate chances per tier." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                            <MudText>PMC Side Plate Chances</MudText>
                        </MudTooltip>
                    </MudItem>
                    <MudItem xs="7">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudNumericField @bind-Value="pmcSidePlateChance[0]" Label="Tier 1" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="pmcSidePlateChance[1]" Label="Tier 2" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="pmcSidePlateChance[2]" Label="Tier 3" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="pmcSidePlateChance[3]" Label="Tier 4" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="pmcSidePlateChance[4]" Label="Tier 5" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="pmcSidePlateChance[5]" Label="Tier 6" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="pmcSidePlateChance[6]" Label="Tier 7" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                        </MudPaper>
                    </MudItem>

                    <MudItem xs="2" />
                    <MudItem xs="3">
                        <MudTooltip Text="Sets Scav front/back plate chances per tier." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                            <MudText>Scav Main Plate Chances</MudText>
                        </MudTooltip>
                    </MudItem>
                    <MudItem xs="7">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudNumericField @bind-Value="scavMainPlateChance[0]" Label="Tier 1" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="scavMainPlateChance[1]" Label="Tier 2" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="scavMainPlateChance[2]" Label="Tier 3" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="scavMainPlateChance[3]" Label="Tier 4" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="scavMainPlateChance[4]" Label="Tier 5" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="scavMainPlateChance[5]" Label="Tier 6" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                            <MudNumericField @bind-Value="scavMainPlateChance[6]" Label="Tier 7" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
                        </MudPaper>
                    </MudItem>

                    <MudItem xs="2" />
                    <MudItem xs="3">
                        <MudTooltip Text="Sets Scav left/right plate chances per tier." Color="Color.Secondary" Placement="Placement.Right" Arrow="true">
                            <MudText>Scav Side Plate Chances</MudText>
                        </MudTooltip>
                    </MudItem>
                    <MudItem xs="7">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudNumericField @bind-Value="scavSidePlateChance[0]" Label="Tier 1" Variant="Variant.Filled" Min="0" Max="100" Disabled="!Data.enableCustomPlateChances" />
