@page "/scav"
@inherits LayoutComponentBase
@inject ISnackbar Snackbar
@using static Core.DataLoader;

<style>
    .mud-divider {
        border-width: 2.5px;
    }
</style>

<MudMainContent>
    <MudContainer Class="mb-20">
        <MudExpansionPanels MultiExpansion="true">
            <MudExpansionPanel Class="mud-expansion-panels-borders ma-3 pa-5" Text="Scav Specific Configuration" Expanded=true>
                <MudGrid>

                    <MudItem xs="1">
                        <MudPaper Class="d-flex justify-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                <ChildContent>
                                    <MudIconButton Icon="@Icons.Material.Filled.Warning" Size="Size.Small" />
                                </ChildContent>
                                <TooltipContent>
                                    <MudText Typo="Typo.caption">All settings on this page, and any other setting that may impact these bots are controlled by this toggle.</MudText><MudSpacer />
                                    <MudSpacer />
                                    <MudText Typo="Typo.caption">EXCEPTION: Normalize Health is always applied if enabled but not excluded.</MudText>
                                </TooltipContent>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="4">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Grants APBS control over Scav Bots. You will have vanilla versions of these bots if disabled." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                <MudText>Scav Bot Generation</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="6">
                        <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                            <MudSwitch T="bool" @bind-Value="_scavEnabled" Color="Color.Success" UncheckedColor="Color.Default" LabelPlacement="Placement.End">@_scavEnabled</MudSwitch>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        @if (MainLayout.pendingChanges.Contains("_scavEnabled") && MainLayout._appUndoButtonToggle)
                        {
                            <MudButton @onclick="undoScavBots" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                        }
                        else if (_scavEnabled != true && MainLayout._appDefaultButtonToggle)
                        {
                            <MudButton @onclick="defaultScavBots" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                        }
                    </MudItem>

                </MudGrid>
            </MudExpansionPanel>
            @if (_scavEnabled)
            {
                <MudExpansionPanel Class="mud-expansion-panels-borders ma-3 pa-5" Style="padding-bottom:25px" Text="Scav Specific Options" Expanded=true>
                    <MudGrid>

                        <MudItem xs="1" />
                        <MudItem xs="4">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="Allows Scav Bots to spawn with additional loot." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Loot</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="6">
                            <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                                <MudSwitch T="bool" @bind-Value="@_scavLootEnabled" Color="Color.Success" UncheckedColor="Color.Default" LabelPlacement="Placement.End">@_scavLootEnabled</MudSwitch>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            @if (MainLayout.pendingChanges.Contains("_scavLootEnabled") && MainLayout._appUndoButtonToggle || MainLayout.pendingChanges.Contains("_scavLootBlacklist") && MainLayout._appUndoButtonToggle)
                            {
                                <MudButton @onclick="undoLoot" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                            }
                            else if (_scavLootEnabled != true && MainLayout._appDefaultButtonToggle || _scavLootBlacklist.Count > 0 && MainLayout._appDefaultButtonToggle)
                            {
                                <MudButton @onclick="defaultLoot" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                            }
                        </MudItem>

                        <MudItem xs="1">
                            <MudPaper Class="d-flex flex-column align-center justify-center mud-width-full py-2">
                                <MudTooltip Text="Only valid MongoIDs are accepted. Enter the ID of the weapon you want blacklisted, then click the arrow (or press enter) to add it to the blacklist." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudIconButton Icon="@Icons.Material.Filled.Warning" Size="Size.Small" />
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="4" Class="align-center">
                            <MudTextField T="string" @bind-Value="@_lootBlacklistEntryHolder"
                                          Typo="Typo.body2" HelperText="Enter valid MongoID"
                                          Placeholder="ItemID"
                                          Variant="Variant.Text"
                                          OnKeyDown="@((e) => {
                                                    if (HandleKeyDown(e))
                                                    {
                                                        if (CanAddMongoIDToList(_lootBlacklistEntryHolder, _scavLootBlacklist))
                                                        {
                                                            _scavLootBlacklist.Add(_lootBlacklistEntryHolder);
                                                            Utils.UpdateView(_scavLootBlacklist, OriginalConfig.scavBots.lootConfig.blacklist, "_scavLootBlacklist");
                                                        } else {
                                                            Utils.UpdateView(_scavLootBlacklist, OriginalConfig.scavBots.lootConfig.blacklist, "_scavLootBlacklist");
                                                        }
                                                    }
                                                } )"
                                          Validation="@(new Func<string, IEnumerable<string>>(Utils.StringObjectIDValidation))"
                                          Immediate="true"
                                          Adornment="Adornment.End"
                                          AdornmentIcon="@Icons.Material.Filled.ArrowRight"
                                          OnAdornmentClick="@(() => {
                                                    if (CanAddMongoIDToList(_lootBlacklistEntryHolder, _scavLootBlacklist))
                                                    {
                                                        _scavLootBlacklist.Add(_lootBlacklistEntryHolder);
                                                        Utils.UpdateView(_scavLootBlacklist, OriginalConfig.scavBots.lootConfig.blacklist, "_scavLootBlacklist");
                                                    } else {
                                                        Utils.UpdateView(_scavLootBlacklist, OriginalConfig.scavBots.lootConfig.blacklist, "_scavLootBlacklist");
                                                    }
                                                } )" />

                        </MudItem>
                        <MudItem xs="6">
                            @foreach (var item in _scavLootBlacklist)
                            {
                                <MudChip Value="@item" T="string" Color="Color.Default" OnClose="@(() => {
                                        _scavLootBlacklist.Remove(item);
                                        Utils.UpdateView(_scavLootBlacklist, OriginalConfig.scavBots.lootConfig.blacklist, "_scavLootBlacklist");
                                    })">@item</MudChip>
                            }
                        </MudItem>

                        <MudDivider DividerType="DividerType.FullWidth" Class="my-3" />

                        <MudItem xs="1" />
                        <MudItem xs="4">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="Allows Scavs to roll their selected round from their weapons caliber, max one time per each magazine." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Roll Ammunition Selection Again</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="6">
                            <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                                <MudSwitch T="bool" @bind-Value="_scavWeaponAmmunitionRollEnabled" Color="Color.Success" UncheckedColor="Color.Default" LabelPlacement="Placement.End">@_scavWeaponAmmunitionRollEnabled</MudSwitch>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            @if (MainLayout._appUndoButtonToggle && (
                           MainLayout.pendingChanges.Contains("_scavWeaponAmmunitionRollEnabled") ||
                           MainLayout.pendingChanges.Contains("_scavWeaponAmmunitionRollChance")))
                            {
                                <MudButton @onclick="undoRollAmmunitionEnabled" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                            }
                            else if (MainLayout._appDefaultButtonToggle && (
                            _scavWeaponAmmunitionRollEnabled != true ||
                            _scavWeaponAmmunitionRollChance != 50))
                            {
                                <MudButton @onclick="defaultRollAmmunitionEnabled" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                            }
                        </MudItem>

                        <MudItem xs="2" />
                        <MudItem xs="3">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="This is the chance that Scavs will roll for a different round from the same caliber, max one time per each magazine." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Roll Again Chance</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            <MudButton Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small" Disabled="!_scavWeaponAmmunitionRollEnabled">@_scavWeaponAmmunitionRollChance</MudButton>
                        </MudItem>
                        <MudItem xs="5">
                            <MudSlider @bind-Value="_scavWeaponAmmunitionRollChance" ValueLabel="true" Min="1" Max="100" Color="Color.Info" Size="Size.Medium" Disabled="!_scavWeaponAmmunitionRollEnabled" />
                        </MudItem>
                        <MudItem xs="1" />

                        <MudItem xs="1" />
                        <MudItem xs="4">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="Allows Scavs to top load their extra magazines." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Toploading</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="6">
                            <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                                <MudSwitch T="bool" @bind-Value="_scavWeaponToploadEnabled" Color="Color.Success" UncheckedColor="Color.Default" LabelPlacement="Placement.End">@_scavWeaponToploadEnabled</MudSwitch>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            @if (MainLayout._appUndoButtonToggle && (
                           MainLayout.pendingChanges.Contains("_scavWeaponToploadEnabled") ||
                           MainLayout.pendingChanges.Contains("_scavWeaponToploadChance") ||
                           MainLayout.pendingChanges.Contains("_scavWeaponToploadPercent")))
                            {
                                <MudButton @onclick="undoToploadEnabled" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                            }
                            else if (MainLayout._appDefaultButtonToggle && (
                            _scavWeaponToploadEnabled != false ||
                            _scavWeaponToploadChance != 30 ||
                            _scavWeaponToploadPercent != 30))
                            {
                                <MudButton @onclick="defaultToploadEnabled" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                            }
                        </MudItem>

                        <MudItem xs="2" />
                        <MudItem xs="3">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="This is the chance that Scavs will topload their extra magazines." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Topload Chance</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            <MudButton Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small" Disabled="!_scavWeaponToploadEnabled">@_scavWeaponToploadChance</MudButton>
                        </MudItem>
                        <MudItem xs="5">
                            <MudSlider @bind-Value="_scavWeaponToploadChance" ValueLabel="true" Min="1" Max="100" Color="Color.Info" Size="Size.Medium" Disabled="!_scavWeaponToploadEnabled" />
                        </MudItem>
                        <MudItem xs="1" />

                        <MudItem xs="2" />
                        <MudItem xs="3">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="This is the percentage of their extra magazine that Scavs will topload." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Topload Percent</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            <MudButton Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small" Disabled="!_scavWeaponToploadEnabled">@_scavWeaponToploadPercent</MudButton>
                        </MudItem>
                        <MudItem xs="5">
                            <MudSlider @bind-Value="_scavWeaponToploadPercent" ValueLabel="true" Min="10" Max="50" Color="Color.Info" Size="Size.Medium" Disabled="!_scavWeaponToploadEnabled" />
                        </MudItem>
                        <MudItem xs="1" />

                        <MudItem xs="1" />
                        <MudItem xs="4">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="Allows random resource consumption on Food/Drink/Medical Items." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Resource Randomization</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="6">
                            <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                                <MudSwitch T="bool" @bind-Value="_scavResourceRandomizationEnabled" Color="Color.Success" UncheckedColor="Color.Default" LabelPlacement="Placement.End">@_scavResourceRandomizationEnabled</MudSwitch>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            @if (
                           MainLayout.pendingChanges.Contains("_scavResourceRandomizationEnabled") && MainLayout._appUndoButtonToggle ||
                           MainLayout.pendingChanges.Contains("_scavMedRateMaxChance") && MainLayout._appUndoButtonToggle ||
                           MainLayout.pendingChanges.Contains("_scavMedRateUsagePercent") && MainLayout._appUndoButtonToggle ||
                           MainLayout.pendingChanges.Contains("_scavFoodRateMaxChance") && MainLayout._appUndoButtonToggle ||
                           MainLayout.pendingChanges.Contains("_scavFoodRateUsagePercent") && MainLayout._appUndoButtonToggle)
                            {
                                <MudButton @onclick="undoResourceRandomization" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                            }
                            else if (
                            _scavResourceRandomizationEnabled != true && MainLayout._appDefaultButtonToggle ||
                            _scavMedRateMaxChance != 10 && MainLayout._appDefaultButtonToggle ||
                            _scavMedRateUsagePercent != 10 && MainLayout._appDefaultButtonToggle ||
                            _scavFoodRateMaxChance != 10 && MainLayout._appDefaultButtonToggle ||
                            _scavFoodRateUsagePercent != 10 && MainLayout._appDefaultButtonToggle)
                            {
                                <MudButton @onclick="defaultResourceRandomization" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                            }
                        </MudItem>

                        <MudItem xs="2" />
                        <MudItem xs="3">
                            <MudTooltip Text="Medical & Food full chances and minimum usage percent." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                <MudText>Medical & Food Values</MudText>
                            </MudTooltip>
                        </MudItem>
                        <MudItem xs="3">
                            <MudPaper Class="d-flex align-start mud-width-full" Style="margin-top: -20px;" Elevation="0">
                                <MudTooltip Text="Chance for medical items to be full resource usage." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudNumericField Margin="Margin.Dense" HideSpinButtons="true" @bind-Value="_scavMedRateMaxChance" Label="Full Chance" HelperText="Medical" Variant="Variant.Filled" Min="1" Max="100" Disabled="!_scavResourceRandomizationEnabled" />
                                </MudTooltip>
                                <MudTooltip Text="Lowest percent possible for a medical item's remaining usage to be." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudNumericField Margin="Margin.Dense" HideSpinButtons="true" @bind-Value="_scavMedRateUsagePercent" Label="Minimum %" HelperText="Medical" Variant="Variant.Filled" Min="1" Max="100" Disabled="!_scavResourceRandomizationEnabled" />
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="3">
                            <MudPaper Class="d-flex align-start mud-width-full" Style="margin-top: -20px;" Elevation="0">
                                <MudTooltip Text="Chance for food/drink items to be full resource usage." Color="Color.Primary" Placement="Placement.Left" Arrow="true">
                                    <MudNumericField Margin="Margin.Dense" HideSpinButtons="true" @bind-Value="_scavFoodRateMaxChance" Label="Full Chance" HelperText="Food/Drink" Variant="Variant.Filled" Min="1" Max="100" Disabled="!_scavResourceRandomizationEnabled" />
                                </MudTooltip>
                                <MudTooltip Text="Lowest percent possible for a food/drink item's remaining usage to be." Color="Color.Primary" Placement="Placement.Left" Arrow="true">
                                    <MudNumericField Margin="Margin.Dense" HideSpinButtons="true" @bind-Value="_scavFoodRateUsagePercent" Label="Minimum %" HelperText="Food/Drink" Variant="Variant.Filled" Min="1" Max="100" Disabled="!_scavResourceRandomizationEnabled" />
                                </MudTooltip>
                                <MudSpacer />
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1" />

                        <MudItem xs="1" />
                        <MudItem xs="4">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="Allows configurable weapon durability." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Weapon Durability Changes</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="6">
                            <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                                <MudSwitch T="bool" @bind-Value="_scavWeaponDurabilityEnabled" Color="Color.Success" UncheckedColor="Color.Default" LabelPlacement="Placement.End">@_scavWeaponDurabilityEnabled</MudSwitch>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            @if (MainLayout._appUndoButtonToggle && (
                                   MainLayout.pendingChanges.Contains("_scavWeaponDurabilityEnabled") ||
                                   MainLayout.pendingChanges.Contains("_scavWeaponDurabilityMin") ||
                                   MainLayout.pendingChanges.Contains("_scavWeaponDurabilityMax") ||
                                   MainLayout.pendingChanges.Contains("_scavWeaponDurabilityMinDelta") ||
                                   MainLayout.pendingChanges.Contains("_scavWeaponDurabilityMaxDelta") ||
                                   MainLayout.pendingChanges.Contains("_scavWeaponDurabilityEnhancementChance")))
                            {
                                <MudButton @onclick="undoWeaponDurability" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                            }
                            else if (MainLayout._appDefaultButtonToggle && (
                                    _scavWeaponDurabilityEnabled != true ||
                                    _scavWeaponDurabilityMin != 70 ||
                                    _scavWeaponDurabilityMax != 90 ||
                                    _scavWeaponDurabilityMinDelta != 5 ||
                                    _scavWeaponDurabilityMaxDelta != 20 ||
                                    _scavWeaponDurabilityEnhancementChance != 1))
                            {
                                <MudButton @onclick="defaultWeaponDurability" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                            }
                        </MudItem>

                        <MudItem xs="2" />
                        <MudItem xs="3">
                            <MudTooltip Text="Adjust weapon durability values." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                <MudText>Weapon Durability Values</MudText>
                            </MudTooltip>
                        </MudItem>
                        <MudItem xs="2">
                            <MudPaper Class="d-flex align-start mud-width-full" Style="margin-top: -20px;" Elevation="0">
                                <MudTooltip Text="Lowest a weapon's 'non-repairable' durability can be." Color="Color.Primary" Placement="Placement.Bottom" Arrow="true">
                                    <MudNumericField Margin="Margin.Dense" HideSpinButtons="true" @bind-Value="_scavWeaponDurabilityMin" Label="Min" HelperText="Durability" Variant="Variant.Filled"
                                                     Min="0"
                                                     Max="_scavWeaponDurabilityMax"
                                                     Disabled="!_scavWeaponDurabilityEnabled" />
                                </MudTooltip>
                                <MudTooltip Text="Highest a weapon's 'non-repairable' durability can be." Color="Color.Primary" Placement="Placement.Bottom" Arrow="true">
                                    <MudNumericField Margin="Margin.Dense"
                                                     HideSpinButtons="true"
                                                     @bind-Value="_scavWeaponDurabilityMax"
                                                     Label="Max"
                                                     HelperText="Durability"
                                                     Variant="Variant.Filled"
                                                     Min="_scavWeaponDurabilityMin"
                                                     Max="100"
                                                     Disabled="!_scavWeaponDurabilityEnabled" />
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="2">
                            <MudPaper Class="d-flex align-start mud-width-full" Style="margin-top: -20px;" Elevation="0">
                                <MudTooltip Text="Lowest possible difference from the rolled 'non-repairable' durabilty to the 'current' durability." Color="Color.Primary" Placement="Placement.Bottom" Arrow="true">
                                    <MudNumericField Margin="Margin.Dense"
                                                     HideSpinButtons="true"
                                                     @bind-Value="_scavWeaponDurabilityMinDelta"
                                                     Label="Min"
                                                     HelperText="Delta"
                                                     Variant="Variant.Filled"
                                                     Min="0"
                                                     Max="100"
                                                     Validation="@(new Func<int, bool>(ValidateDelta))"
                                                     Disabled="!_scavWeaponDurabilityEnabled" />
                                </MudTooltip>
                                <MudTooltip Text="Highest possible difference from the rolled 'non-repairable' durabilty to the 'current' durability." Color="Color.Primary" Placement="Placement.Bottom" Arrow="true">
                                    <MudNumericField Margin="Margin.Dense"
                                                     HideSpinButtons="true"
                                                     @bind-Value="_scavWeaponDurabilityMaxDelta"
                                                     Label="Max"
                                                     HelperText="Delta"
                                                     Variant="Variant.Filled"
                                                     Min="0"
                                                     Max="100"
                                                     Validation="@(new Func<int, bool>(ValidateDelta))"
                                                     Disabled="!_scavWeaponDurabilityEnabled" />
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="2">
                            <MudPaper Class="d-flex align-start mud-width-full" Style="margin-top: -20px;" Elevation="0">
                                <MudTooltip Text="Lowest Possible Current Durability Roll." Color="Color.Primary" Placement="Placement.Bottom" Arrow="true">
                                    <MudNumericField Disabled Margin="Margin.Dense" HideSpinButtons="true" @bind-Value="_scavWeaponDurabilityMinLimitPercent" Label="Min" HelperText="Actual" Variant="Variant.Filled"
                                                     Min="0"
                                                     Max="100" />
                                </MudTooltip>
                                <MudTooltip Text="Highest Possible Current Durability Roll." Color="Color.Primary" Placement="Placement.Bottom" Arrow="true">
                                    <MudNumericField Disabled Margin="Margin.Dense" HideSpinButtons="true" @bind-Value="_scavWeaponDurabilityMaxLimitPercent" Label="Max" HelperText="Actual" Variant="Variant.Filled"
                                                     Min="0"
                                                     Max="100" />
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1" />

                        <MudItem xs="2" />
                        <MudItem xs="3">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="This is the chance that bot weapons have an enhancement applied to them." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Enhancement Chance</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            <MudButton Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">@_scavWeaponDurabilityEnhancementChance</MudButton>
                        </MudItem>
                        <MudItem xs="6">
                            <MudSlider @bind-Value="_scavWeaponDurabilityEnhancementChance" ValueLabel="true" Min="0" Max="100" Color="Color.Info" Size="Size.Medium" Disabled="!_scavWeaponDurabilityEnabled" />
                        </MudItem>

                        <MudItem xs="1" />
                        <MudItem xs="4">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="Adds all keys to existing scav loot pool." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Add All Keys</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="6">
                            <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                                <MudSwitch T="bool" Disabled="(_scavAddKeycards || _scavAddMechanicalKeys) ? true : false" @bind-Value="_scavAddAllKeys" Color="Color.Success" UncheckedColor="Color.Default" LabelPlacement="Placement.End">@_scavAddAllKeys</MudSwitch>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
