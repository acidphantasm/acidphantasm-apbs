@page "/boss"
@inherits LayoutComponentBase
@inject ISnackbar Snackbar
@using static Core.DataLoader;

<style>
    .mud-divider {
    border-width: 2.5px;
    }
</style>

<MudMainContent>
    <MudContainer Class="mb-20">
        <MudExpansionPanels MultiExpansion="true">
            <MudExpansionPanel Class="mud-expansion-panels-borders ma-3 pa-5" Text="Boss Specific Configuration" Expanded=true>
                <MudGrid>

                    <MudItem xs="1">
                        <MudPaper Class="d-flex justify-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                <ChildContent>
                                    <MudIconButton Icon="@Icons.Material.Filled.Warning" Size="Size.Small" />
                                </ChildContent>
                                <TooltipContent>
                                    <MudText Typo="Typo.caption">All settings on this page, and any other setting that may impact these bots are controlled by this toggle.</MudText><MudSpacer />
                                    <MudSpacer />
                                    <MudText Typo="Typo.caption">EXCEPTION: Normalize Health is always applied if enabled but not excluded.</MudText>
                                </TooltipContent>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="4">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Grants APBS control over Bosses (including Goons). You will have vanilla versions of these bots if disabled." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                <MudText>Boss Bot Generation</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="6">
                        <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                            <MudSwitch T="bool" @bind-Value="_bossEnabled" Color="Color.Success" UncheckedColor="Color.Default" LabelPlacement="Placement.End">@_bossEnabled</MudSwitch>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        @if (MainLayout.pendingChanges.Contains("_bossEnabled") && MainLayout._appUndoButtonToggle)
                        {
                            <MudButton @onclick="undoBossBots" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                        }
                        else if (_bossEnabled != true && MainLayout._appDefaultButtonToggle)
                        {
                            <MudButton @onclick="defaultBossBots" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                        }
                    </MudItem>

                </MudGrid>
            </MudExpansionPanel>
            @if (_bossEnabled)
            {
                <MudExpansionPanel Class="mud-expansion-panels-borders ma-3 pa-5" Text="Boss Specific Options" Expanded=true>
                    <MudGrid>

                        <MudItem xs="1" />
                        <MudItem xs="4">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="Allows Boss Bots to spawn with additional loot." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Loot</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="6">
                            <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                                <MudSwitch T="bool" @bind-Value="@_bossLootEnabled" Color="Color.Success" UncheckedColor="Color.Default" LabelPlacement="Placement.End">@_bossLootEnabled</MudSwitch>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            @if (MainLayout.pendingChanges.Contains("_bossLootEnabled") && MainLayout._appUndoButtonToggle || MainLayout.pendingChanges.Contains("_bossLootBlacklist") && MainLayout._appUndoButtonToggle)
                            {
                                <MudButton @onclick="undoLoot" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                            }
                            else if (_bossLootEnabled != true && MainLayout._appDefaultButtonToggle || _bossLootBlacklist.Count > 0 && MainLayout._appDefaultButtonToggle)
                            {
                                <MudButton @onclick="defaultLoot" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                            }
                        </MudItem>

                        <MudItem xs="1">
                            <MudPaper Class="d-flex flex-column align-center justify-center mud-width-full py-2">
                                <MudTooltip Text="Only valid MongoIDs are accepted. Enter the ID of the weapon you want blacklisted, then click the arrow (or press enter) to add it to the blacklist." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudIconButton Icon="@Icons.Material.Filled.Warning" Size="Size.Small" />
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="4" Class="align-center">
                            <MudTextField T="string" @bind-Value="@_lootBlacklistEntryHolder"
                            Typo="Typo.body2" HelperText="Enter valid MongoID"
                            Placeholder="ItemID"
                            Variant="Variant.Text"
                            OnKeyDown="@((e) => {
                                                    if (HandleKeyDown(e))
                                                    {
                                                        if (CanAddMongoIDToList(_lootBlacklistEntryHolder, _bossLootBlacklist))
                                                        {
                                                            _bossLootBlacklist.Add(_lootBlacklistEntryHolder);
                                                            Utils.UpdateView(_bossLootBlacklist, OriginalConfig.bossBots.lootConfig.blacklist, "_bossLootBlacklist");
                                                        } else {
                                                            Utils.UpdateView(_bossLootBlacklist, OriginalConfig.bossBots.lootConfig.blacklist, "_bossLootBlacklist");
                                                        }
                                                    }
                                                } )"
                            Validation="@(new Func<string, IEnumerable<string>>(Utils.StringObjectIDValidation))"
                            Immediate="true"
                            Adornment="Adornment.End"
                            AdornmentIcon="@Icons.Material.Filled.ArrowRight"
                            OnAdornmentClick="@(() => {
                                                    if (CanAddMongoIDToList(_lootBlacklistEntryHolder, _bossLootBlacklist))
                                                    {
                                                        _bossLootBlacklist.Add(_lootBlacklistEntryHolder);
                                                        Utils.UpdateView(_bossLootBlacklist, OriginalConfig.bossBots.lootConfig.blacklist, "_bossLootBlacklist");
                                                    } else {
                                                        Utils.UpdateView(_bossLootBlacklist, OriginalConfig.bossBots.lootConfig.blacklist, "_bossLootBlacklist");
                                                    }
                                                } )" />

                        </MudItem>
                        <MudItem xs="6">
                            @foreach (var item in _bossLootBlacklist)
                            {
                                <MudChip Value="@item" T="string" Color="Color.Default" OnClose="@(() => {
                                        _bossLootBlacklist.Remove(item);
                                        Utils.UpdateView(_bossLootBlacklist, OriginalConfig.bossBots.lootConfig.blacklist, "_bossLootBlacklist");
                                    })">@item</MudChip>
                            }
                        </MudItem>

                        <MudDivider DividerType="DividerType.FullWidth" Class="my-3" />

                        <MudItem xs="1" />
                        <MudItem xs="4">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="Allows Bosses to roll their selected round from their weapons caliber, max one time per each magazine." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Roll Ammunition Selection Again</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="6">
                            <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                                <MudSwitch T="bool" @bind-Value="_bossWeaponAmmunitionRollEnabled" Color="Color.Success" UncheckedColor="Color.Default" LabelPlacement="Placement.End">@_bossWeaponAmmunitionRollEnabled</MudSwitch>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            @if (MainLayout._appUndoButtonToggle && (
                                   MainLayout.pendingChanges.Contains("_bossWeaponAmmunitionRollEnabled") ||
                                   MainLayout.pendingChanges.Contains("_bossWeaponAmmunitionRollChance")))
                            {
                                <MudButton @onclick="undoRollAmmunitionEnabled" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                            }
                            else if (MainLayout._appDefaultButtonToggle && (
                                    _bossWeaponAmmunitionRollEnabled != true ||
                                    _bossWeaponAmmunitionRollChance != 50))
                            {
                                <MudButton @onclick="defaultRollAmmunitionEnabled" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                            }
                        </MudItem>

                        <MudItem xs="2" />
                        <MudItem xs="3">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="This is the chance that Bosses will roll for a different round from the same caliber, max one time per each magazine." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Roll Again Chance</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            <MudButton Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small" Disabled="!_bossWeaponAmmunitionRollEnabled">@_bossWeaponAmmunitionRollChance</MudButton>
                        </MudItem>
                        <MudItem xs="5">
                            <MudSlider @bind-Value="_bossWeaponAmmunitionRollChance" ValueLabel="true" Min="1" Max="100" Color="Color.Info" Size="Size.Medium" Disabled="!_bossWeaponAmmunitionRollEnabled" />
                        </MudItem>
                        <MudItem xs="1" />

                        <MudItem xs="1" />
                        <MudItem xs="4">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="Allows Bosses to top load their extra magazines." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Toploading</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="6">
                            <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                                <MudSwitch T="bool" @bind-Value="_bossWeaponToploadEnabled" Color="Color.Success" UncheckedColor="Color.Default" LabelPlacement="Placement.End">@_bossWeaponToploadEnabled</MudSwitch>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            @if (MainLayout._appUndoButtonToggle && (
                           MainLayout.pendingChanges.Contains("_bossWeaponToploadEnabled") ||
                           MainLayout.pendingChanges.Contains("_bossWeaponToploadChance") ||
                           MainLayout.pendingChanges.Contains("_bossWeaponToploadPercent")))
                            {
                                <MudButton @onclick="undoToploadEnabled" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                            }
                            else if (MainLayout._appDefaultButtonToggle && (
                            _bossWeaponToploadEnabled != false ||
                            _bossWeaponToploadChance != 30 ||
                            _bossWeaponToploadPercent != 30))
                            {
                                <MudButton @onclick="defaultToploadEnabled" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                            }
                        </MudItem>

                        <MudItem xs="2" />
                        <MudItem xs="3">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="This is the chance that Bosses will topload their extra magazines." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Topload Chance</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            <MudButton Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small" Disabled="!_bossWeaponToploadEnabled">@_bossWeaponToploadChance</MudButton>
                        </MudItem>
                        <MudItem xs="5">
                            <MudSlider @bind-Value="_bossWeaponToploadChance" ValueLabel="true" Min="1" Max="100" Color="Color.Info" Size="Size.Medium" Disabled="!_bossWeaponToploadEnabled" />
                        </MudItem>
                        <MudItem xs="1" />

                        <MudItem xs="2" />
                        <MudItem xs="3">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="This is the percentage of their extra magazine that Bosses will topload." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Topload Percent</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            <MudButton Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small" Disabled="!_bossWeaponToploadEnabled">@_bossWeaponToploadPercent</MudButton>
                        </MudItem>
                        <MudItem xs="5">
                            <MudSlider @bind-Value="_bossWeaponToploadPercent" ValueLabel="true" Min="10" Max="50" Color="Color.Info" Size="Size.Medium" Disabled="!_bossWeaponToploadEnabled" />
                        </MudItem>
                        <MudItem xs="1" />

                        <MudItem xs="1" />
                        <MudItem xs="4">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="Allows random resource consumption on Food/Drink/Medical Items." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Resource Randomization</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="6">
                            <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                                <MudSwitch T="bool" @bind-Value="_bossResourceRandomizationEnabled" Color="Color.Success" UncheckedColor="Color.Default" LabelPlacement="Placement.End">@_bossResourceRandomizationEnabled</MudSwitch>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            @if (
                              MainLayout.pendingChanges.Contains("_bossResourceRandomizationEnabled") && MainLayout._appUndoButtonToggle ||
                              MainLayout.pendingChanges.Contains("_bossMedRateMaxChance") && MainLayout._appUndoButtonToggle ||
                              MainLayout.pendingChanges.Contains("_bossMedRateUsagePercent") && MainLayout._appUndoButtonToggle ||
                              MainLayout.pendingChanges.Contains("_bossFoodRateMaxChance") && MainLayout._appUndoButtonToggle ||
                              MainLayout.pendingChanges.Contains("_bossFoodRateUsagePercent") && MainLayout._appUndoButtonToggle)
                            {
                                <MudButton @onclick="undoResourceRandomization" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                            }
                            else if (
                                _bossResourceRandomizationEnabled != true && MainLayout._appDefaultButtonToggle ||
                                _bossMedRateMaxChance != 50 && MainLayout._appDefaultButtonToggle ||
                                _bossMedRateUsagePercent != 50 && MainLayout._appDefaultButtonToggle ||
                                _bossFoodRateMaxChance != 50 && MainLayout._appDefaultButtonToggle ||
                                _bossFoodRateUsagePercent != 50 && MainLayout._appDefaultButtonToggle)
                            {
                                <MudButton @onclick="defaultResourceRandomization" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                            }
                        </MudItem>

                        <MudItem xs="2" />
                        <MudItem xs="3">
                            <MudTooltip Text="Medical & Food full chances and minimum usage percent." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                <MudText>Medical & Food Values</MudText>
                            </MudTooltip>
                        </MudItem>
                        <MudItem xs="3">
                            <MudPaper Class="d-flex align-start mud-width-full" Style="margin-top: -20px;" Elevation="0">
                                <MudTooltip Text="Chance for medical items to be full resource usage." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudNumericField Margin="Margin.Dense" HideSpinButtons="true" @bind-Value="_bossMedRateMaxChance" Label="Full Chance" HelperText="Medical" Variant="Variant.Filled" Min="1" Max="100" Disabled="!_bossResourceRandomizationEnabled" />
                                </MudTooltip>
                                <MudTooltip Text="Lowest percent possible for a medical item's remaining usage to be." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudNumericField Margin="Margin.Dense" HideSpinButtons="true" @bind-Value="_bossMedRateUsagePercent" Label="Minimum %" HelperText="Medical" Variant="Variant.Filled" Min="1" Max="100" Disabled="!_bossResourceRandomizationEnabled" />
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="3">
                            <MudPaper Class="d-flex align-start mud-width-full" Style="margin-top: -20px;" Elevation="0">
                                <MudTooltip Text="Chance for food/drink items to be full resource usage." Color="Color.Primary" Placement="Placement.Left" Arrow="true">
                                    <MudNumericField Margin="Margin.Dense" HideSpinButtons="true" @bind-Value="_bossFoodRateMaxChance" Label="Full Chance" HelperText="Food/Drink" Variant="Variant.Filled" Min="1" Max="100" Disabled="!_bossResourceRandomizationEnabled" />
                                </MudTooltip>
                                <MudTooltip Text="Lowest percent possible for a food/drink item's remaining usage to be." Color="Color.Primary" Placement="Placement.Left" Arrow="true">
                                    <MudNumericField Margin="Margin.Dense" HideSpinButtons="true" @bind-Value="_bossFoodRateUsagePercent" Label="Minimum %" HelperText="Food/Drink" Variant="Variant.Filled" Min="1" Max="100" Disabled="!_bossResourceRandomizationEnabled" />
                                </MudTooltip>
                                <MudSpacer />
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1" />

                        <MudItem xs="1" />
                        <MudItem xs="4">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="Allows configurable weapon durability." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Weapon Durability Changes</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="6">
                            <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                                <MudSwitch T="bool" @bind-Value="_bossWeaponDurabilityEnabled" Color="Color.Success" UncheckedColor="Color.Default" LabelPlacement="Placement.End">@_bossWeaponDurabilityEnabled</MudSwitch>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            @if (MainLayout._appUndoButtonToggle && (
                                 MainLayout.pendingChanges.Contains("_bossWeaponDurabilityEnabled") ||
                                 MainLayout.pendingChanges.Contains("_bossWeaponDurabilityMin") ||
                                 MainLayout.pendingChanges.Contains("_bossWeaponDurabilityMax") ||
                                 MainLayout.pendingChanges.Contains("_bossWeaponDurabilityMinDelta") ||
                                 MainLayout.pendingChanges.Contains("_bossWeaponDurabilityMaxDelta") ||
                                 MainLayout.pendingChanges.Contains("_bossWeaponDurabilityEnhancementChance")))
                            {
                                <MudButton @onclick="undoWeaponDurability" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                            }
                            else if (MainLayout._appDefaultButtonToggle && (
                                    _bossWeaponDurabilityEnabled != true ||
                                    _bossWeaponDurabilityMin != 90 ||
                                    _bossWeaponDurabilityMax != 100 ||
                                    _bossWeaponDurabilityMinDelta != 0 ||
                                    _bossWeaponDurabilityMaxDelta != 5 ||
                                    _bossWeaponDurabilityEnhancementChance != 0))
                            {
                                <MudButton @onclick="defaultWeaponDurability" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                            }
                        </MudItem>

                        <MudItem xs="2" />
                        <MudItem xs="3">
                            <MudTooltip Text="Adjust weapon durability values." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                <MudText>Weapon Durability Values</MudText>
                            </MudTooltip>
                        </MudItem>
                        <MudItem xs="2">
                            <MudPaper Class="d-flex align-start mud-width-full" Style="margin-top: -20px;" Elevation="0">
                                <MudTooltip Text="Lowest a weapon's 'non-repairable' durability can be." Color="Color.Primary" Placement="Placement.Bottom" Arrow="true">
                                    <MudNumericField Margin="Margin.Dense" HideSpinButtons="true" @bind-Value="_bossWeaponDurabilityMin" Label="Min" HelperText="Durability" Variant="Variant.Filled" 
                                    Min="0" 
                                    Max="_bossWeaponDurabilityMax"
                                    Disabled="!_bossWeaponDurabilityEnabled" />
                                </MudTooltip>
                                <MudTooltip Text="Highest a weapon's 'non-repairable' durability can be." Color="Color.Primary" Placement="Placement.Bottom" Arrow="true">
                                    <MudNumericField 
                                    Margin="Margin.Dense" 
                                    HideSpinButtons="true" 
                                    @bind-Value="_bossWeaponDurabilityMax" 
                                    Label="Max" 
                                    HelperText="Durability" 
                                    Variant="Variant.Filled" 
                                    Min="_bossWeaponDurabilityMin"
                                    Max="100"
                                    Disabled="!_bossWeaponDurabilityEnabled" />
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="2">
                            <MudPaper Class="d-flex align-start mud-width-full" Style="margin-top: -20px;" Elevation="0">
                                <MudTooltip Text="Lowest possible difference from the rolled 'non-repairable' durabilty to the 'current' durability." Color="Color.Primary" Placement="Placement.Bottom" Arrow="true">
                                    <MudNumericField 
                                    Margin="Margin.Dense" 
                                    HideSpinButtons="true" 
                                    @bind-Value="_bossWeaponDurabilityMinDelta" 
                                    Label="Min" 
                                    HelperText="Delta" 
                                    Variant="Variant.Filled"
                                    Min="0"
                                    Max="100"
                                    Validation="@(new Func<int, bool>(ValidateDelta))"
                                    Disabled="!_bossWeaponDurabilityEnabled" />
                                </MudTooltip>
                                <MudTooltip Text="Highest possible difference from the rolled 'non-repairable' durabilty to the 'current' durability." Color="Color.Primary" Placement="Placement.Bottom" Arrow="true">
                                    <MudNumericField 
                                    Margin="Margin.Dense" 
                                    HideSpinButtons="true" 
                                    @bind-Value="_bossWeaponDurabilityMaxDelta" 
                                    Label="Max" 
                                    HelperText="Delta" 
                                    Variant="Variant.Filled"
                                    Min="0"
                                    Max="100"
                                    Validation="@(new Func<int, bool>(ValidateDelta))"
                                    Disabled="!_bossWeaponDurabilityEnabled" />
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="2">
                            <MudPaper Class="d-flex align-start mud-width-full" Style="margin-top: -20px;" Elevation="0">
                                <MudTooltip Text="Lowest Possible Current Durability Roll." Color="Color.Primary" Placement="Placement.Bottom" Arrow="true">
                                    <MudNumericField Disabled Margin="Margin.Dense" HideSpinButtons="true" @bind-Value="_bossWeaponDurabilityMinLimitPercent" Label="Min" HelperText="Actual" Variant="Variant.Filled"
                                    Min="0" 
                                    Max="100"  />
                                </MudTooltip>
                                <MudTooltip Text="Highest Possible Current Durability Roll." Color="Color.Primary" Placement="Placement.Bottom" Arrow="true">
                                    <MudNumericField Disabled Margin="Margin.Dense" HideSpinButtons="true" @bind-Value="_bossWeaponDurabilityMaxLimitPercent" Label="Max" HelperText="Actual" Variant="Variant.Filled"
                                    Min="0" 
                                    Max="100" />
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1" />

                        <MudItem xs="2" />
                        <MudItem xs="3">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="This is the chance that bot weapons have an enhancement applied to them." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Enhancement Chance</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            <MudButton Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">@_bossWeaponDurabilityEnhancementChance</MudButton>
                        </MudItem>
                        <MudItem xs="6">
                            <MudSlider @bind-Value="_bossWeaponDurabilityEnhancementChance" ValueLabel="true" Min="0" Max="100" Color="Color.Info" Size="Size.Medium" Disabled="!_bossWeaponDurabilityEnabled"/>
                        </MudItem>

                    </MudGrid>
                </MudExpansionPanel>
                


                <MudExpansionPanel Class="mud-expansion-panels-borders ma-3 pa-5" Style="padding-bottom:25px" Text="Plate Class Selection" Expanded=true>
                    <MudGrid>
                        <MudItem xs="1" />
                        <MudItem xs="11" Style="margin-top: 20px;">
                            <MudTooltip Text="Adjust the plate class levels for Tier 1. If a plate is selected that cannot fit in the armour, APBS will upgrade the plate level by 1 until it is valid." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                <MudText>Tier 1 (Lv1-10)</MudText>
                            </MudTooltip>
                        </MudItem>

                        <MudItem xs="2" />
                        <MudItem xs="3" Style="margin-top: 20px;">
                            <MudTooltip Text="Adjust the plate class levels for Tier 1. If a plate is selected that cannot fit in the armour, APBS will upgrade the plate level by 1 until it is valid." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
